Checks: '-*,bugprone-*,cppcoreguidelines*,misc*,modernize-*,readability-identifier-naming,-misc-include-cleaner,-modernize-use-trailing-return-type,-bugprone-easily-swappable-parameters,-cppcoreguidelines-avoid-const-or-ref-data-members,-misc-non-private-member-variables-in-classes'

ExcludeHeaderFilterRegex: '.*\/lib\/.*'
HeaderFilterRegex: '.*\/src\/.*'

ExtraArgsBefore: 
 - '-std=c++23'

# ExtraArgs:
#  - 'Werror'

CheckOptions: [
  {key: readability-identifier-naming.VaribleCase, value: camelBack},
  {key: readability-identifier-naming.ClassMethodCase, value: lower_case},
  {key: readability-identifier-naming.FunctionCase, value: lower_case},
]
